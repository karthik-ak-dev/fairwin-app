# Environment variables template
# Copy this to .env.local and fill in actual values

# AWS DynamoDB Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
DYNAMODB_TABLE_PREFIX=massivehikecoin_

# Blockchain Configuration (BSC)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_project_id
NEXT_PUBLIC_USDT_CONTRACT_ADDRESS=0x55d398326f99059fF775485246999027B3197955
NEXT_PUBLIC_DEPOSIT_WALLET_ADDRESS=your_deposit_wallet
WITHDRAWAL_WALLET_PRIVATE_KEY=your_withdrawal_wallet_private_key
NEXT_PUBLIC_BSC_RPC_URL=https://bsc-dataseed.binance.org/

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_here

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
STAKE_MIN_AMOUNT=50
STAKE_MAX_AMOUNT=10000
MONTHLY_RETURN_RATE=0.08
STAKE_DURATION_MONTHS=24
